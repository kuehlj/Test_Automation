{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "http://www.theprogrampdx.com/work"
    },
    {
      "type": "setWindowSize",
      "width": "",
      "height": "5000"
    },
    {
      "type": "verifyTextPresent",
      "text": "Constantly curious, we solve creative challenges with technology, story and strategy."
    },
    {
      "type": "verifyTextPresent",
      "text": "Our latest work"
    },
    {
      "type": "mouseOverElement",
      "locator": {
        "type": "xpath",
        "value": "//section[3]/div[1]/a/div[2]/div"
      }
    },
    {
      "type": "assertTextPresent",
      "text": "Nike Basketball"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "xpath",
        "value": "//section[3]/div[1]/a/div[2]/div/h2"
      }
    },
    {
      "type": "pause",
      "waitTime": ""
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "WORK"
      }
    },
    {
      "type": "verifyTitle",
      "title": "Work | The Program."
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}